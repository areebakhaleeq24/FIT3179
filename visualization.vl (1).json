{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 620,
  "height": 300,
  "data": {"url": "https://areebakhaleeq24.github.io/FIT3179/outbreaks.csv"},
  "title": "Spread of illnesses over the months",
  "config": {"view": {"strokeWidth": 0, "step": 13}, "axis": {"domain": false}},
  "mark": "rect",
  "encoding": {
    "x": {"field": "Year", "type": "ordinal", "title": "Year"},
    "y": {"field": "Month", "type": "ordinal", "title": "Month"},
    "color": {
      "field": "Illnesses",
      "aggregate": "sum",
      "type": "quantitative",
      "legend": {"title": null}
    }
  }
}