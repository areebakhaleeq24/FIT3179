{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "description": "No, of illneses over time",
    "data": {
    "url": "https://areebakhaleeq24.github.io/FIT3179/outbreaks.csv"
  },
    "width": "container",
    "mark": {
      "type": "line",
      "interpolate": "monotone"
    },
    "encoding": {
      "x": {
        "field": "Year",
      },
      "y": {
        "field": "Illnesses",
        "aggregate": "count"
      },
      "color": {
        "type": "nominal",
        "field": "Year",
      }
    },
    "config": {}
  }
